<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashActivity"
    android:background="@drawable/adimage">

    <ImageView
        android:id="@+id/icon"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/aaab"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="80dp"/>
    <TextView
        android:id="@+id/appName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Online Shop"
        android:textColor="#6236AF"
        android:textStyle="bold"
        android:textSize="26sp"
        app:layout_constraintTop_toBottomOf="@id/icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"/>
    <ImageView
        android:id="@+id/image1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:rotation="@integer/material_motion_duration_long_2"/>
    <ImageView
        android:id="@+id/image2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaae"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/image1"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="55dp"
        android:rotation="@integer/material_motion_duration_long_1"/>
    <ImageView
        android:id="@+id/image3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        android:rotation="@integer/material_motion_duration_short_1"/>
    <ImageView
        android:id="@+id/image4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaae"
        app:layout_constraintTop_toBottomOf="@id/image1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/image2"
        android:layout_marginTop="80dp"
        android:rotation="@integer/material_motion_duration_medium_1"/>
    <ImageView
        android:id="@+id/image5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintTop_toBottomOf="@id/image2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/image4"
        android:layout_marginStart="80dp"
        android:layout_marginTop="100dp"
        android:rotation="@integer/material_motion_duration_long_2"/>

    <ImageView
        android:id="@+id/image6"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintTop_toBottomOf="@id/image1"
        app:layout_constraintEnd_toStartOf="@id/icon"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="250dp"
        android:rotation="@integer/material_motion_duration_short_1"/>
    <ImageView
        android:id="@+id/image7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaae"
        app:layout_constraintTop_toBottomOf="@id/image5"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="10dp"
        android:rotation="@integer/material_motion_duration_short_1"/>
    <ImageView
        android:id="@+id/image8"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaae"
        app:layout_constraintTop_toBottomOf="@id/image6"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="170dp"
        android:layout_marginStart="10dp"
        android:rotation="@integer/material_motion_duration_long_1"/>
    <ImageView
        android:id="@+id/image9"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintTop_toBottomOf="@id/image7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/icon"
        android:layout_marginTop="150dp"
        android:rotation="@integer/material_motion_duration_medium_1"/>
    <ImageView
        android:id="@+id/image10"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaae"
        app:layout_constraintTop_toBottomOf="@id/icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="150dp"
        android:rotation="@integer/material_motion_duration_short_1"/>
    <ImageView
        android:id="@+id/image11"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="20dp"
        android:rotation="@integer/material_motion_duration_long_1"/>
    <ImageView
        android:id="@+id/image12"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/aaac"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="@dimen/_30sdp"
        android:layout_marginBottom="40dp"
        android:rotation="@integer/material_motion_duration_short_2"/>
</androidx.constraintlayout.widget.ConstraintLayout>